<div class="Events_container">
  <h1>Upcoming Events</h1>
    <% @events.each do |event| %>
      <div class="Events_card">
        <div class="Events_content">
          <h3>Event: <%= event.name %></h3>
          <h3>Description: <%= event.description %></h3>
          <h3>Date: <%= event.date %></h3>
        </div>
      </div>
    <% end %>
  </div>
</div>